[Connection]

#Connection type could be:
#   prompt: ask every time hostname, username, password
#   config: load configuration file (specifying it on SSHConfigFile)
#       and use one of these hosts (specifying it on ConfigFileHost)
ConnectionType = config

#If choose config in ConnectionType, specify config file
SSHConfigFile = ~/.ssh/config

#If choose config in ConnectionType, specify connecting host
ConfigFileHost = Nustrake

[topsecret.server.com]
Port = 50022
ForwardX11 = no