[Connection]

# Connection type could be:
#    prompt:    ask every time hostname, username, password
#    config:    load configuration file (specifying it on SSHConfigFile)
#               and use one of these hosts (specifying it on ConfigFileHost)
#    command:   use a ssh command (specifying it on SSHCommand)
ConnectionType = config

# If choose config in ConnectionType, specify config file
SSHConfigFile = ~/.ssh/config

# If choose config in ConnectionType, specify connecting host
ConfigFileHost = Foxtrot

# If choose command in ConnectionType, set the command
#SSHCommand = ssh username@hostname:port -o IdentityFile="priv"

[VNCServer]

# Specifies the starting port to be used by VNC servers
StartingPort = 5900

# Specifies if have to use security options 
SecureServer = yes

# Specifies the max remote window number
# The max number of remote windows is 10
MaxWindows = 10

[topsecret.server.com]
Port = 50022
ForwardX11 = no